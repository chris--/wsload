{
    "name" : "wsload",
    "version" : "0.1.0",
    "description" : "Load Testing Application for massive concurrent connections and complex scripted testcases",
    "main" : "Wsload.js",
    "directories" : {
	"node_modules" : "node_modules",
        "lib" : "lib",
        "example" : "example",
        "test" : "test"
    },
    "dependencies": {
        "mongodb" : "*"
    },
    "devDependencies"   : {
       "nodeunit" : "*"
    },
    "scripts" : {
      "test": "$(which nodeunit || echo node_modules/nodeunit/bin/nodeunit) test/unit/scripts.js"
    },
    "repository" : {
        "type" : "git",
        "url" : "http://github.com/chris--/wsload.git"
    },
    "keywords" : [
        "benchmark",
        "timing",
        "time",
        "stopwatch",
        "load testing"
    ],
    "author" : {
        "name" : "Christian Vogt",
        "email" : "mail@christianvogt.de",
        "url" : "http://christianvogt.de"
    },
    "license" : "MIT/X11",
    "engine" : { "node" : ">=0.8" }
}
